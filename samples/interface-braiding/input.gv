digraph G {
  // Teams (fibred lanes): Eng:{A,B,C}, Ops:{D,E}, Finance:{F}
  // Interfaces are cross-lane edges that create churn.
  subgraph cluster_Eng { label="Eng"; A; B; C; }
  subgraph cluster_Ops { label="Ops"; D; E; }
  subgraph cluster_Fin { label="Finance"; F; }

  A -> B; B -> C;           // intra-lane
  C -> D [label="handoff"]; // cross-lane
  D -> E; E -> F [label="cost"]; // cross-lane
}
